{
  "crons": [
    {
      "path": "/api/agent-cron",
      "schedule": "0 12 * * *"
    }
  ],
  "functions": {
    "api/*.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/((?!api/|manifest.json|favicon.ico|images/).*)",
      "destination": "/index.html"
    }
  ]
}